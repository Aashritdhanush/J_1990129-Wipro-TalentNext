class PrimeNum 
{
	public static void main(String[] args) 
	{
		int n=11;
		int flag=0;
		int X=n/2;
		for(int i=2;i<X;i++)
		{
			if(n%i==0)
			{
				flag=1;
				break;
			}
		}
		if(flag==0)
		{
			System.out.println("the number is prime number");
		}
		else
		{
			System.out.println("the number is not a prime number");
		}
	}
}
