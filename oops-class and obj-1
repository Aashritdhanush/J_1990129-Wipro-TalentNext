class Box
{
	int width;
	int height;
	int depth;

	Box(int w,int h,int d)
	{
		width=w;
		height=h;
		depth=d;
	}
	/*int getwidth()
	{
		return width;
	}
	int getheight()
	{
		return height;
	}
	int getdepth()
	{
		return depth;
	}*/
	int volume()
	{
		int vol=width*height*depth;
		return vol;
	}
}
class LaunchBox
{
	public static void main(String[] args) 
	{
		Box b=new Box(5,4,3);
		System.out.println("volume of the cube is"+b.volume());
	}
}
