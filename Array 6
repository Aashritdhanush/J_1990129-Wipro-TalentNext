class Larg2Array
{
	public static void main(String[] args) 
	{
		int a[]={5,2,7,9,1,8};
		int b[]=new int[a.length];
		int small=a[0];
		for(int i=0;i<a.length;i++)
		{
			for(int j=i+1;j<a.length;j++)
			{
				if(a[i]>a[j])
				{
					int temp=a[i];
					a[i]=a[j];
					a[j]=temp;

				}
			}
		}
		for(int i=0;i<a.length;i++)
		{
				System.out.print(a[i]+" ");
		}
		System.out.println();
		for(int i=0;i<2;i++)
		{
			System.out.print(a[i]+" ");
		}
		System.out.println();
	}
}
