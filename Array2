class Array2 
{
	public static void main(String[] args) 
	{
		int arr[]={6,7,5,3,2,1};
		int big=arr[0];
		int	small=arr[0];
		for(int i=0;i<arr.length;i++)
		{
				if(small>arr[i])
				{
					small=arr[i];
				}
				else
				{
					big=arr[i];
				}
		}
		System.out.println("smaller element is"+small);
		System.out.println("bigger element is"+big);
	}
}
